      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "types_constants_code_v3"
      },
      "outputs": [],
      "source": [
        "class DocumentCategory(Enum):\n",
        "    IDENTITE = \"Justificatif d'identité\"\n",
        "    SALAIRE = \"Bulletins de salaire\"\n",
        "    IMPOT = \"Avis d'imposition\"\n",
        "    COMPTE = \"Relevés de comptes\"\n",
        "    EPARGNE = \"Justificatif d'épargne\"\n",
        "    TRAVAIL = \"Contrat de travail\"\n",
        "    UNKNOWN = \"Unknown\"\n",
        "\n",
        "class ConfidenceScore(Enum):\n",
        "    HIGH = \"High\"\n",
        "    MEDIUM = \"Medium\"\n",
        "    LOW = \"Low\"\n",
        "\n",
        "# Helper function for validating confidence scores\n",
        "def _validate_confidence_enum_from_str(v: Any) -> Optional[ConfidenceScore]:\n",
        "    if v is None:\n",
        "        return None\n",
        "    if isinstance(v, str):\n",
        "        try:\n",
        "            return ConfidenceScore(v)\n",
        "        except ValueError:\n",
        "            return None\n",
        "    if isinstance(v, ConfidenceScore):\n",
        "        return v\n",
        "    return None\n",
        "\n",
        "class ExtractedMetadataPydantic(BaseModel):\n",
        "    emprunteur: Optional[str] = None\n",
        "    emprunteurConfidence: Optional[ConfidenceScore] = None\n",
        "    mois: Optional[List[str]] = None\n",
        "    moisConfidence: Optional[ConfidenceScore] = None\n",
        "    annee: Optional[List[str]] = None\n",
        "    anneeConfidence: Optional[ConfidenceScore] = None\n",
        "\n",
        "    @validator('emprunteurConfidence', 'moisConfidence', 'anneeConfidence', pre=True, allow_reuse=True)\n",
        "    def _validate_confidence_fields(cls, v):\n",
        "        return _validate_confidence_enum_from_str(v)\n",
        "    \n",
        "    @validator('mois', 'annee', pre=True, allow_reuse=True)\n",
        "    def _ensure_list_or_none(cls, v, field):\n",
        "        \"\"\"Ensures that 'mois' and 'annee' fields are lists of strings or None.\n",
        "        If a single value is provided, it's converted to a list containing that value as a string.\n",
        "        Non-convertible types or items that are None within a list are handled gracefully.\n",
        "        \"\"\"\n",
        "        if v is None:\n",
        "            return None\n",
        "        if not isinstance(v, list):\n",
        "            # Attempt to convert non-list scalar values to a list of one string item\n",
        "            return [str(v)] if isinstance(v, (str, int, float)) else None\n",
        "        # Ensure all items in the list are strings, filtering out any None items\n",
        "        return [str(item) for item in v if item is not None]\n",
        "\n",
        "class GeminiAnalysisResponsePydantic(BaseModel):\n",
        "    category: DocumentCategory\n",
        "    categoryConfidence: Optional[ConfidenceScore] = None\n",
        "    metadata: ExtractedMetadataPydantic\n",
        "\n",
        "    @validator('category', pre=True, allow_reuse=True)\n",
        "    def _validate_category_str_to_enum(cls, v):\n",
        "        \"\"\"Converts a string value to a DocumentCategory enum member.\n",
        "        Tries direct mapping (value), then by name (key usually uppercase), and defaults to UNKNOWN.\n",
        "        \"\"\"\n",
        "        if isinstance(v, str):\n",
        "            try:\n",
        "                # Attempt direct match by enum value (e.g., \"Justificatif d'identité\")\n",
        "                return DocumentCategory(v)\n",
            except ValueError:\n",
        "                try:\n",
        "                    # Attempt to match by enum key (e.g., \"IDENTITE\" or \"SALAIRE\")\n",
        "                    return DocumentCategory[v.upper()]\n",
        "                except KeyError:\n",
        "                    # If no match is found by value or key, default to UNKNOWN\n",
        "                    return DocumentCategory.UNKNOWN\n",
        "        if isinstance(v, DocumentCategory):\n",
        "            return v # Already the correct enum type\n",
        "        # If not a string or DocumentCategory enum, default to UNKNOWN\n",
        "        return DocumentCategory.UNKNOWN\n",
        "\n",
        "    @validator('categoryConfidence', pre=True, allow_reuse=True)\n",
        "    def _validate_category_confidence_enum(cls, v):\n",
        "        return _validate_confidence_enum_from_str(v)\n",
        "\n",
        "print(\"Définitions des types et constantes (Pydantic) chargées.\") # User feedback in notebook context"
      ]
    },
