      "source": [
        "## 4. Fonction d'Analyse de Document avec Gemini"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "analysis_func_code_v3"
      },
      "outputs": [],
      "source": [
        "def analyze_document_with_gemini_v3(\n",
        "    file_path: Optional[str] = None,\n",
        "    file_bytes: Optional[bytes] = None,\n",
        "    file_name_for_mime: Optional[str] = None,\n",
        "    provided_mime_type: Optional[str] = None\n",
        ") -> tuple[Optional[GeminiAnalysisResponsePydantic], str]:\n",
        "    \"\"\"\n",
        "    Analyse un document (PDF ou image) en utilisant l'API Gemini.\n",
        "    Retourne un tuple: (objet GeminiAnalysisResponsePydantic ou None, réponse brute de Gemini).\n",
        "    \"\"\"\n",
        "    actual_mime_type = None\n",
        "    actual_file_bytes = None\n",
        "    raw_response_text = \"No API call made or API call failed before receiving a response.\"\n",
        "\n",
        "    try:\n",
        "        # Determine file bytes and MIME type, whether from path or direct bytes\n",
        "        if file_path:\n",
        "            if not os.path.exists(file_path):\n",
        "                raise FileNotFoundError(f\"Fichier non trouvé: {file_path}\")\n",
        "            with open(file_path, \"rb\") as f:\n",
        "                actual_file_bytes = f.read()\n",
        "            # Guess MIME type from file path, fallback to provided_mime_type\n",
        "            actual_mime_type = provided_mime_type or mimetypes.guess_type(file_path)[0]\n",
        "            if not file_name_for_mime: \n",
        "                file_name_for_mime = os.path.basename(file_path)\n",
        "        elif file_bytes:\n",
        "            actual_file_bytes = file_bytes\n",
        "            actual_mime_type = provided_mime_type\n",
        "            if not actual_mime_type and file_name_for_mime:\n",
        "                # Guess MIME type from filename if provided with bytes\n",
        "                actual_mime_type = mimetypes.guess_type(file_name_for_mime)[0]\n",
        "        else:\n",
        "             raise ValueError(\"ERREUR INTERNE: Soit file_path, soit file_bytes doit être fourni.\")\n",
        "\n",
        "        if not actual_mime_type and file_name_for_mime:\n",
        "            ext = os.path.splitext(file_name_for_mime)[1].lower()\n",
        "            mime_map = {\n",
        "                \".pdf\": \"application/pdf\", \".jpg\": \"image/jpeg\", \".jpeg\": \"image/jpeg\",\n",
        "                \".png\": \"image/png\", \".webp\": \"image/webp\", \".heic\": \"image/heic\", \".heif\": \"image/heif\"\n",
        "            }\n",
        "            # Fallback MIME type map for common extensions if mimetypes.guess_type fails or isn't sufficient\n",
        "            actual_mime_type = mime_map.get(ext)\n",
        "\n",
        "        if not actual_file_bytes or not actual_mime_type:\n",
        "            raise ValueError(\n",
        "                f\"Impossible de déterminer les bytes du fichier ou le type MIME. \"\n",
        "                f\"Fichier: {file_name_for_mime or file_path or 'inconnu'}, Type MIME: {actual_mime_type}\"\n",
        "            )\n",
        "\n",
        "        # If the document is an image, perform a basic integrity check\n",
        "        if actual_mime_type.startswith(\"image/\"):\n",
        "            try:\n",
        "                img = Image.open(io.BytesIO(actual_file_bytes))\n",
        "                img.verify() # Basic check for image integrity\n",
        "            except Exception as img_e:\n",
        "                raise ValueError(f\"Fichier image invalide ou corrompu {file_name_for_mime or ''}: {img_e}\")\n",
        "\n",
        "        user_prompt = get_base_prompt_python()\n",
        "        document_part = {\"mime_type\": actual_mime_type, \"data\": actual_file_bytes}\n",
        "        \n",
        "        model = genai.GenerativeModel(\n",
        "            MODEL_NAME,\n",
        "            system_instruction=SYSTEM_INSTRUCTION_PYTHON\n",
        "        )\n",
        "        generation_config = genai.types.GenerationConfig(\n",
        "            temperature=0.1,\n",
        "            response_mime_type=\"application/json\"\n",
        "        )\n",
        "        contents = [user_prompt, document_part]\n",
        "\n",
        "        display_filename = file_name_for_mime or file_path or \"unknown_file\"\n",
        "        # User feedback: Log the start of the analysis for the current document\n",
        "        print(\n",
        "            f\"INFO: Analyse du document '{os.path.basename(display_filename)}' \"\n",
        "            f\"(MIME: {actual_mime_type}, Taille: {len(actual_file_bytes)} bytes) en cours avec {MODEL_NAME}...\"\n",
        "        )\n",
        "\n",
        "        # Configure API request timeout from environment variable or use default\n",
        "        DEFAULT_REQUEST_TIMEOUT = 300  # Default timeout in seconds\n",
        "        try:\n",
        "            REQUEST_TIMEOUT = int(os.environ.get(\"GEMINI_REQUEST_TIMEOUT\", str(DEFAULT_REQUEST_TIMEOUT)))\n",
        "        except ValueError:\n",
        "            print(\n",
        "                f\"AVERTISSEMENT: La valeur de GEMINI_REQUEST_TIMEOUT ('{os.environ.get('GEMINI_REQUEST_TIMEOUT')}') \"\n",
