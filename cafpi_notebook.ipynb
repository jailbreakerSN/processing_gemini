      "source": [
        "# Structure AppAnalysisResult Python pour stocker les résultats finaux par fichier\n",
        "class AppAnalysisResultPython(BaseModel):\n",
        "    fileName: str\n",
        "    category: Optional[DocumentCategory] = None \n",
        "    categoryConfidence: Optional[ConfidenceScore] = None\n",
        "    metadata: Optional[ExtractedMetadataPydantic] = None\n",
        "    rawResponse: Optional[str] = None \n",
        "    error: Optional[str] = None\n",
        "\n",
        "    # Validateur pour s'assurer que category et categoryConfidence sont bien des Enums si fournis\n",
        "    @validator('category', 'categoryConfidence', pre=True, allow_reuse=True)\n",
        "    def _validate_enums(cls, v, field):\n",
        "        if v is None: return None\n",
        "        if field.name == 'category':\n",
        "            if isinstance(v, str):\n",
        "                try: return DocumentCategory(v) \n",
        "                except ValueError: \n",
        "                    try: return DocumentCategory[v.upper()] \n",
        "                    except KeyError: return DocumentCategory.UNKNOWN\n",
        "            return v if isinstance(v, DocumentCategory) else DocumentCategory.UNKNOWN\n",
        "        elif field.name == 'categoryConfidence':\n",
        "            if isinstance(v, str): \n",
        "                try: return ConfidenceScore(v)\n",
        "                except ValueError: return None\n",
        "            return v if isinstance(v, ConfidenceScore) else None\n",
        "        return v\n",
        "\n",
        "# --- Configuration des fichiers de test ---\n",
        "sample_docs_dir_v2 = \"sample_docs_v2\"\n",
        "if not os.path.exists(sample_docs_dir_v2):\n",
        "    os.makedirs(sample_docs_dir_v2)\n",
        "    print(f\"Dossier '{sample_docs_dir_v2}' créé. Veuillez y ajouter des fichiers de test.\")\n",
        "\n",
        "sample_file_names_v2 = [\n",
        "    # Ajoutez les noms de vos fichiers de test ici, ex: \"mon_bulletin_de_paie.pdf\"\n",
        "]\n",
        "\n",
        "# Téléchargement d'un fichier PDF d'exemple\n",
        "example_pdf_url_v2 = \"https://www.w3.org/WAI/ER/tests/xhtml/testfiles/resources/pdf/dummy.pdf\"\n",
        "example_pdf_name_v2 = \"dummy_test_v2.pdf\"\n",
        "example_pdf_path_v2 = os.path.join(sample_docs_dir_v2, example_pdf_name_v2)\n",
        "\n",
        "if not sample_file_names_v2 and not os.path.exists(example_pdf_path_v2):\n",
        "    try:\n",
        "        print(f\"Téléchargement du fichier d'exemple depuis {example_pdf_url_v2}...\")\n",
        "        response_dl = requests.get(example_pdf_url_v2, timeout=30)\n",
        "        response_dl.raise_for_status()\n",
        "        with open(example_pdf_path_v2, \"wb\") as f: f.write(response_dl.content)\n",
        "        print(f\"Fichier d'exemple téléchargé : {example_pdf_path_v2}\")\n",
        "        sample_file_names_v2.append(example_pdf_name_v2)\n",
        "    except requests.exceptions.RequestException as e:\n",
        "        print(f\"Impossible de télécharger le fichier d'exemple: {e}\")\n",
        "elif not sample_file_names_v2 and os.path.exists(example_pdf_path_v2):\n",
        "    sample_file_names_v2.append(example_pdf_name_v2)\n",
        "\n",
        "all_app_results = []\n",
        "\n",
        "if not sample_file_names_v2:\n",
        "    display(Markdown(\"⚠️  **Aucun fichier à analyser.** Ajoutez des noms de fichiers dans `sample_file_names_v2` \\n et placez les fichiers correspondants dans le dossier `sample_docs_dir_v2`.\"))\n",
        "else:\n",
        "    for file_name in sample_file_names_v2:\n",
        "        file_path_to_analyze = os.path.join(sample_docs_dir_v2, file_name)\n",
        "        display(Markdown(f\"\\n--- Analyse du fichier : `{file_name}` ---\"))\n",
        "        \n",
        "        app_result_data = {\"fileName\": file_name} # Initialiser avec le nom du fichier\n",
        "\n",
        "        parsed_gemini_response, raw_text_response = analyze_document_with_gemini_v2(file_path=file_path_to_analyze, file_name_for_mime=file_name)\n",
        "        app_result_data[\"rawResponse\"] = raw_text_response\n",
        "\n",
        "        if parsed_gemini_response:\n",
        "            app_result_data[\"category\"] = parsed_gemini_response.category\n",
        "            app_result_data[\"categoryConfidence\"] = parsed_gemini_response.categoryConfidence\n",
        "            app_result_data[\"metadata\"] = parsed_gemini_response.metadata\n",
        "        else:\n",
        "            # L'erreur est déjà dans raw_text_response si analyze_document_with_gemini_v2 retourne None pour la partie parsée\n",
        "            app_result_data[\"error\"] = raw_text_response \n            app_result_data[\"category\"] = DocumentCategory.UNKNOWN\n",
        "            app_result_data[\"categoryConfidence\"] = ConfidenceScore.LOW\n",
        "            app_result_data[\"metadata\"] = ExtractedMetadataPydantic() # Métadonnées vides en cas d'erreur majeure\n",
        "        \n",
        "        # Créer l'objet AppAnalysisResultPython final\n",
        "        final_app_result = AppAnalysisResultPython(**app_result_data)\n",
        "        all_app_results.append(final_app_result)\n",
        "\n",
