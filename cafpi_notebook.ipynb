        "## 2. Définitions des Types et Constantes (basées sur `types.ts` et `constants.ts`)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "types_constants_code_v2"
      },
      "outputs": [],
      "source": [
        "class DocumentCategory(Enum):\n",
        "    IDENTITE = \"Justificatif d'identité\"\n",
        "    SALAIRE = \"Bulletins de salaire\"\n",
        "    IMPOT = \"Avis d'imposition\"\n",
        "    COMPTE = \"Relevés de comptes\"\n",
        "    EPARGNE = \"Justificatif d'épargne\"\n",
        "    TRAVAIL = \"Contrat de travail\"\n",
        "    UNKNOWN = \"Unknown\"\n",
        "\n",
        "class ConfidenceScore(Enum):\n",
        "    HIGH = \"High\"\n",
        "    MEDIUM = \"Medium\"\n",
        "    LOW = \"Low\"\n",
        "\n",
        "class ExtractedMetadataPydantic(BaseModel):\n",
        "    emprunteur: Optional[str] = None\n",
        "    emprunteurConfidence: Optional[ConfidenceScore] = None\n",
        "    mois: Optional[List[str]] = None # Doit être une liste de chaînes ou null\n",
        "    moisConfidence: Optional[ConfidenceScore] = None\n",
        "    annee: Optional[List[str]] = None # Doit être une liste de chaînes ou null\n",
        "    anneeConfidence: Optional[ConfidenceScore] = None\n",
        "\n",
        "    @validator('emprunteurConfidence', 'moisConfidence', 'anneeConfidence', pre=True)\n",
        "    def _validate_confidence_str_to_enum(cls, v):\n",
        "        if v is None: return None\n",
        "        if isinstance(v, str):\n",
        "            try:\n",
        "                return ConfidenceScore(v)\n",
        "            except ValueError:\n",
        "                print(f\"AVERTISSEMENT: Valeur de confiance '{v}' invalide, utilisation de None.\")\n",
        "                return None\n",
        "        if isinstance(v, ConfidenceScore): return v\n",
        "        print(f\"AVERTISSEMENT: Type de confiance '{type(v)}' inattendu pour la valeur '{v}', utilisation de None.\")\n",
        "        return None\n",
        "    \n",
        "    @validator('mois', 'annee', pre=True)\n",
        "    def _ensure_list_or_none(cls, v, field):\n",
        "        if v is None: return None\n",
        "        if not isinstance(v, list):\n",
        "            print(f\"AVERTISSEMENT: Le champ '{field.name}' attendait une liste ou None, reçu {type(v)} ('{v}'). Tentative de conversion.\")\n",
        "            # Tenter une conversion simple si c'est une chaîne unique, sinon None\n",
        "            return [str(v)] if isinstance(v, (str, int, float)) else None\n",
        "        return [str(item) for item in v if item is not None] # S'assurer que les éléments sont des chaînes\n",
        "\n",
        "class GeminiAnalysisResponsePydantic(BaseModel):\n",
        "    category: DocumentCategory\n",
        "    categoryConfidence: Optional[ConfidenceScore] = None\n",
        "    metadata: ExtractedMetadataPydantic\n",
        "\n",
        "    @validator('category', pre=True)\n",
        "    def _validate_category_str_to_enum(cls, v):\n",
        "        if isinstance(v, str):\n",
        "            try:\n",
        "                # Tenter de faire correspondre la valeur de l'enum (ex: \"Justificatif d'identité\")\n",
        "                return DocumentCategory(v)\n",
        "            except ValueError:\n",
        "                # Tenter de faire correspondre la clé de l'enum (ex: \"IDENTITE\") si la valeur échoue\n",
        "                try:\n",
        "                    return DocumentCategory[v.upper()] \n",
        "                except KeyError:\n",
        "                    print(f\"AVERTISSEMENT: Catégorie '{v}' non reconnue, utilisation de UNKNOWN.\")\n",
        "                    return DocumentCategory.UNKNOWN\n",
        "        if isinstance(v, DocumentCategory): return v\n",
        "        print(f\"AVERTISSEMENT: Type de catégorie '{type(v)}' inattendu pour la valeur '{v}', utilisation de UNKNOWN.\")\n",
        "        return DocumentCategory.UNKNOWN\n",
        "\n",
        "    @validator('categoryConfidence', pre=True)\n",
        "    def _validate_cat_confidence_str_to_enum(cls, v):\n",
        "        if v is None: return None\n",
        "        if isinstance(v, str):\n",
        "            try:\n",
        "                return ConfidenceScore(v)\n",
        "            except ValueError:\n",
        "                print(f\"AVERTISSEMENT: Valeur de confiance de catégorie '{v}' invalide, utilisation de None.\")\n",
        "                return None\n",
        "        if isinstance(v, ConfidenceScore): return v\n",
        "        print(f\"AVERTISSEMENT: Type de confiance de catégorie '{type(v)}' inattendu pour la valeur '{v}', utilisation de None.\")\n",
        "        return None\n",
        "\n",
        "print(\"Définitions des types et constantes (Pydantic) chargées.\")"
      ]
    },
