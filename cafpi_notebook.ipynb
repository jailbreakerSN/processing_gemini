        "            display(Markdown(f\"**<font color='red'>Erreur d'analyse :</font>** {final_app_result.error}\"))\n",
        "        if final_app_result.category:\n",
        "            cat_conf_str = f\"(Confiance : `{final_app_result.categoryConfidence.value if final_app_result.categoryConfidence else 'N/A'}`)\"\n",
        "            display(Markdown(f\"**Catégorie Identifiée :** `{final_app_result.category.value}` {cat_conf_str}\"))\n",
        "        \n"
        "        if final_app_result.metadata:\n",
        "            display(Markdown(\"**Métadonnées Extraites :**\"))\n",
        "            meta_table_md = \"| Champ | Valeur | Confiance |\\n|---|---|---|\"\n",
        "            # Afficher emprunteur\n",
        "            if final_app_result.metadata.emprunteur is not None or final_app_result.metadata.emprunteurConfidence is not None:\n",
        "                emp_val = final_app_result.metadata.emprunteur if final_app_result.metadata.emprunteur is not None else \"*N/A*\"\n",
        "                emp_conf = final_app_result.metadata.emprunteurConfidence.value if final_app_result.metadata.emprunteurConfidence else \"*N/A*\"\n",
        "                meta_table_md += f\"\\n| `emprunteur` | {emp_val} | `{emp_conf}` |\"\n",
        "            # Afficher mois\n",
        "            if final_app_result.metadata.mois is not None or final_app_result.metadata.moisConfidence is not None:\n",
        "                mois_val = ', '.join(final_app_result.metadata.mois) if final_app_result.metadata.mois else \"*N/A*\"\n",
        "                mois_conf = final_app_result.metadata.moisConfidence.value if final_app_result.metadata.moisConfidence else \"*N/A*\"\n",
        "                meta_table_md += f\"\\n| `mois` | [{mois_val}] | `{mois_conf}` |\"\n",
        "            # Afficher annee\n",
        "            if final_app_result.metadata.annee is not None or final_app_result.metadata.anneeConfidence is not None:\n",
        "                annee_val = ', '.join(final_app_result.metadata.annee) if final_app_result.metadata.annee else \"*N/A*\"\n",
        "                annee_conf = final_app_result.metadata.anneeConfidence.value if final_app_result.metadata.anneeConfidence else \"*N/A*\"\n",
        "                meta_table_md += f\"\\n| `annee` | [{annee_val}] | `{annee_conf}` |\"\n",
        "            \n",
        "            if meta_table_md.count('\\n') > 1: # S'il y a des lignes de métadonnées\n",
        "                display(Markdown(meta_table_md))\n",
        "            else:\n",
        "                display(Markdown(\"*Aucune métadonnée pertinente extraite selon le schéma.*\"))\n",
        "        \n",
        "        display(Markdown(\"--- Réponse JSON Brute de Gemini (formatée si possible) ---\"))\n",
        "        try:\n",
        "            # Essayer de pretty-print si c'est du JSON valide\n",
        "            json_raw = json.loads(final_app_result.rawResponse if final_app_result.rawResponse else \"{}\")\n",
        "            display(JSON(json_raw))\n",
        "        except:\n",
        "            # Sinon, afficher comme texte brut\n",
        "            display(Markdown(f\"```text\\n{final_app_result.rawResponse or '(Pas de réponse brute)'}\\n```\"))\n",
        "        display(Markdown(\"--- Fin de l'analyse pour ce fichier ---\"))\n",
        "\n",
